<h1> Individual student</h1>
  <div class="student">
  First name: <%= @student.first_name %>
  Last name: <%= @student.last_name %>
  Email: <%= @student.email %>
  Phone: <%= @student.phone_number %>
  Bio: <%= @student.bio %>
  Linkedin: <%= @student.linkedin_url %>
  Twitter: <%= @student.twitter_handle %>
  Personal: <%= @student.personal_url %>
  Resume: <%= @student.resume_url %>
  Github: <%= @student.github_url %>
  Photo: <%= @student.photo %>
 </div> %>

 <% @student.experiences.each do |experience| %>
  <div class="experience">
    Start Date: <%= experience.start_date %>
    End Date: <%= experience.end_date %>
    Title: <%= experience.job_title %>
    Company: <%= experience.company_name %>
    Details: <%= experience.details %>
    </div>
    <% end %>

    <% @student.educations.each do |education| %>
    <div class="education">
    Start Date: <%= education.start_date %>
    End Date: <%= education.end_date %>
    Degree: <%= education.degree %>
    University: <%= education.university_name %>
    Details: <%= education.details %>
    </div>
    <% end %>

    <% @student.skills.each do |skill| %>
    <div class="skills">
      Skills: <%= skill.skill_name %>
    </div>
    <% end %>

    <% @student.capstones.each do |capstone| %>
    <div class="capstone" >
    Capstone: <% capstone.name %>
    Description: <% capstone.description %>
    URL: <% capstone.URL %>
    Screenshot: <% capstone.screenshot %>
    </div>
    <% end %>











